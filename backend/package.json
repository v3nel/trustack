{
    "name": "backend",
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "start": "bun run src/index.ts",
        "db:create": "docker compose -f ./db.docker-compose.yml up -d",
        "db:generate": "bunx drizzle-kit generate",
        "db:migrate": "bunx drizzle-kit migrate",
        "db:push": "bunx drizzle-kit push",
        "db:studio": "bunx drizzle-kit studio",
        "db:seed": "bun run src/db/seed.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@types/bun": "latest",
        "@types/express": "^5.0.6",
        "drizzle-kit": "^0.31.9",
        "eslint": "^10.0.0",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.3.0",
        "jiti": "^2.6.1",
        "typescript-eslint": "^8.55.0"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "drizzle-orm": "^0.45.1",
        "express": "^5.2.1",
        "jose": "^6.1.3",
        "postgres": "^3.4.8"
    }
}
